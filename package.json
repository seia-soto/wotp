{
  "name": "wotp",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Seia-Soto <seia@outlook.kr>",
  "license": "MIT",
  "scripts": {
    "start": "yarn deploy && yarn app:start",
    "debug": "yarn deploy && yarn app:debug",
    "app:start": "DEBUG='wotp' node -r esm scripts/server.js",
    "app:debug": "DEBUG='*' node -r esm scripts/server.js",
    "deploy": "node -r esm scripts/prepare.js",
    "lint": "eslint --ext .js",
    "release": "standard-version",
    "prepare": "husky install",
    "test": "yarn deploy -c && tap --reporter=list"
  },
  "dependencies": {
    "argon2": "^0.28.2",
    "esm": "^3.2.25",
    "fastify": "^3.17.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "mysql2": "^2.2.5",
    "notp": "^2.0.3"
  },
  "devDependencies": {
    "debug": "^4.3.1",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^6.0.0",
    "pino-pretty": "^5.0.2",
    "standard-version": "^9.3.0",
    "tap": "^15.0.9"
  }
}
